{
  "name": "reachinbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec ts-node backend/src/index.ts",
    "dev:frontend": "next dev",
    "build": "npm run build:backend && next build",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node backend/dist/index.js",
    "start:frontend": "next start",
    "lint": "eslint",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@google/generative-ai": "^0.1.3",
    "@slack/webhook": "^7.0.2",
    "axios": "^1.6.2",
    "cohere-ai": "^7.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "imap": "^0.8.19",
    "mailparser": "^3.6.5",
    "next": "16.0.1",
    "pg": "^8.11.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/imap": "^0.8.40",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "nodemon": "^3.0.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
